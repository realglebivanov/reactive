{
  "name": "@realglebivanov/reactive",
  "version": "1.0.7",
  "description": "A simple and permissive observable-driven UI toolkit",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "reactive",
    "observable",
    "ui",
    "toolkit",
    "framework",
    "typescript",
    "no-deps",
    "no-dependencies"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "bunup --filter dist",
    "watch": "bunup --watch --filter dist",
    "demo:build": "bunup --filter demo",
    "demo:watch": "bunup --watch --filter demo",
    "demo:serve": "bun x serve demo/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realglebivanov/reactive.git"
  },
  "author": "realglebivanov <realglebivanov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/realglebivanov/reactive/issues"
  },
  "homepage": "https://github.com/realglebivanov/reactive#readme",
  "devDependencies": {
    "bunup": "^0.16.26",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ]
}
